{
    "name": "benn/php-math",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Ben",
            "email": "ben.voydie64@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^11",
        "friendsofphp/php-cs-fixer": "^3",
        "phpstan/phpstan": "^1"
    },



    "scripts": {
  "test": "phpunit",
  "cs-check": "php-cs-fixer fix --dry-run --diff --verbose",
  "cs-fix": "php-cs-fixer fix --verbose",
  "stan": "phpstan analyse src --level=max"
}

}
